<div class="tab-pane fade" id="list-create-element"  role="tabpanel" aria-labelledby="list-create-element-list">
<%= form_for( :api_admin,url:'/api_admin',remote:true,html: {class: 'create_form'}   ) do %>
  <p><%= text_field_tag 'name',nil,placeholder: 'Insert Element Name', size:30,:required => true %></p>
  <p>Element Type: <%= select_tag 'type', options_for_select(['bulletinBoard','attendanceSheet']) %></p>
  <p>Location X: <%= number_field_tag 'location_x',:elasticity, :step => 0.1 %></p>
  <p>Location Y: <%= number_field_tag 'location_y',:elasticity, :step => 0.1 %></p>
  <p>Add attributes <%= text_field_tag 'attributes',nil,placeholder: "name:data, name:data",size:40 %></p>
  <p>Expiration Date: <%= date_field_tag 'expiration_date' %></p>
  <%= hidden_field_tag(:request_type,'element_action') %>
  <p> <%= submit_tag 'Create Element'%></p>
<% end %>
</div>